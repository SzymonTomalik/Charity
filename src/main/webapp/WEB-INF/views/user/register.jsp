<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><!DOCTYPE html><html lang="pl"><%@include file="/WEB-INF/head.jspf" %><body><%@include file="/WEB-INF/header.jspf" %><section class="register-page">    <h2>Załóż konto</h2>    <form:form modelAttribute="registration" name="registerForm">        <div class="form-group">            <form:input name="firstName" placeholder="Imię" path="firstName"/>            <br>            <form:errors path="firstName" cssStyle="color: red"/>        </div>        <div class="form-group">            <form:input name="lastName" placeholder="Nazwisko" path="lastName"/>            <br>            <form:errors path="lastName" cssStyle="color: red"/>        </div>        <div class="form-group">            <form:input type="email" name="email" placeholder="Email" path="email"/>            <br>            <form:errors path="email" cssStyle="color: red"/>        </div>        <div class="form-group">            <form:input type="password" name="pass1" placeholder="Hasło" path="pass1"/>            <br>            <form:errors path="pass1" cssStyle="color: red"/>        </div>        <div class="form-group">            <form:input type="password" name="pass2" placeholder="Powtórz hasło" path="pass2"/>            <br>            <form:errors path="pass2" cssStyle="color: red"/>        </div>        <div class="form-group form-group--buttons">            <a href="login.html" class="btn btn--without-border">Zaloguj się</a>            <button class="btn" type="submit">Załóż konto</button>        </div>    </form:form></section><%@include file="/WEB-INF/footer.jspf" %></body></html>